{
  "assistant": {
    "default_model": {
      "model": "gpt-4o-mini",
      "provider": "openai"
    },
    "enabled": true,
    "version": "2"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "auto_install_extensions": {
    "json": true,
    "make": true,
    "nix": true,
    "toml": true,
    "zig": true,
    "dockerfile": true,
    "zed-legacy-themes": true,
    "wakatime": true,
    "discord-presence": true
  },
  "format_on_save": false,
  "vim_mode": true,
  "lsp": {
    "nix": {
      "binary": {
        "path_lookup": true
      }
    },
    "zls": {
      "binary": {
        "path_lookup": true
      }
    }
  },
  "terminal": {
    "env": {
      "TERM": "alacritty"
    }
  },
  "theme": {
    "dark": "Rosé Pine",
    "light": "Rosé Pine Dawn",
    "mode": "system"
  },

  "language_overrides": {
    "TypeScript": {
      "format_on_save": {
        "external": {
          "command": "node_modules/.bin/prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "TSX": {
      "format_on_save": {
        "external": {
          "command": "node_modules/.bin/prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    }
  }
}
