{
  "assistant": {
    "default_model": {
      "model": "gpt-4o-mini",
      "provider": "openai"
    },
    "enabled": true,
    "version": "2"
  },
  "auto_install_extensions": {
    "json": true,
    "make": true,
    "nix": true,
    "toml": true,
    "zig": true
  },
  "lsp": {
    "nix": {
      "binary": {
        "path_lookup": true
      }
    },
    "zls": {
      "binary": {
        "path_lookup": true
      }
    }
  },
  "terminal": {
    "env": {
      "TERM": "alacritty"
    }
  },
  "theme": {
    "dark": "Rosé Pine",
    "light": "Rosé Pine Dawn",
    "mode": "system"
  },
  "vim_mode": true
}
